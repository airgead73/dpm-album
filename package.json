{
  "name": "dpm-album",
  "version": "1.0.0",
  "description": "Fullstack application using node, express, cloudinary, gulp, and auth0.",
  "main": ".index.js",
  "scripts": {
    "start": "node index.js",
    "server": "NODE_ENV=development nodemon -r dotenv/config index.js",
    "public": "gulp dev",
    "watch": "concurrently \"npm:public\" \"npm:server \"",
    "build": "gulp",
    "heroku-postbuild": "echo Skip builds on Heroku",
    "deploy": "git push heroku v4:main",
    "log": "heroku logs --tail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airgead73/dpm-album.git"
  },
  "keywords": [
    "node",
    "express",
    "auth0",
    "cloudinary"
  ],
  "author": "dan moneypenny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airgead73/dpm-album/issues"
  },
  "homepage": "https://github.com/airgead73/dpm-album#readme",
  "browserslist": [
    ">1%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "cloudinary": "^1.29.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-openid-connect": "^2.7.2",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "memorystore": "^1.6.7",
    "mongoose": "^6.2.9",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "bootswatch": "^5.1.3",
    "browserify": "^17.0.0",
    "camelcase": "^6.3.0",
    "concurrently": "^7.1.0",
    "del": "^6.0.0",
    "dotenv": "^16.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "nodemon": "^2.0.15",
    "sass": "^1.49.11",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "nodemonConfig": {
    "delay": 2500,
    "ext": "js,json",
    "ignore": [
      "assets/scripts/*.js",
      "assets/scss/*.scss",
      "app/views/*",
      "scss/*",
      "*.ejs"
    ]
  }
}
